<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="Custom Favicon Assets" inherit_id="web.layout">
        <xpath expr="//link[@rel='shortcut icon']" position="replace">
            <link rel="shortcut icon" type="image/x-icon" href="/static_favicon/static/src/img/favicon.ico"/>
        </xpath>
    </template>
</odoo>